<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

  <!-- ═══════════════════════════════════════════════════════════
       STRATA UI – HIGH CONTRAST VARIANT
       Maximum readability – strong borders, bold text
       ═══════════════════════════════════════════════════════════ -->

  <Color x:Key="Palette.Neutral0">#FFFFFF</Color>
  <Color x:Key="Palette.Neutral50">#F0F0F0</Color>
  <Color x:Key="Palette.Neutral100">#E0E0E0</Color>
  <Color x:Key="Palette.Neutral200">#C0C0C0</Color>
  <Color x:Key="Palette.Neutral300">#909090</Color>
  <Color x:Key="Palette.Neutral400">#606060</Color>
  <Color x:Key="Palette.Neutral500">#404040</Color>
  <Color x:Key="Palette.Neutral600">#303030</Color>
  <Color x:Key="Palette.Neutral700">#202020</Color>
  <Color x:Key="Palette.Neutral800">#101010</Color>
  <Color x:Key="Palette.Neutral900">#000000</Color>

  <Color x:Key="Palette.Accent400">#4499FF</Color>
  <Color x:Key="Palette.Accent500">#0066FF</Color>
  <Color x:Key="Palette.Accent600">#0044CC</Color>
  <Color x:Key="Palette.Accent100">#D0E4FF</Color>
  <Color x:Key="Palette.Accent200">#80B3FF</Color>

  <Color x:Key="Palette.Danger400">#FF4444</Color>
  <Color x:Key="Palette.Danger500">#DD0000</Color>
  <Color x:Key="Palette.Danger600">#AA0000</Color>
  <Color x:Key="Palette.Danger100">#FFD0D0</Color>

  <Color x:Key="Palette.Warning400">#FFAA00</Color>
  <Color x:Key="Palette.Warning500">#CC8800</Color>
  <Color x:Key="Palette.Warning100">#FFF0CC</Color>

  <Color x:Key="Palette.Success400">#00CC44</Color>
  <Color x:Key="Palette.Success500">#009933</Color>
  <Color x:Key="Palette.Success100">#CCFFD8</Color>

  <Color x:Key="Color.Background">#000000</Color>
  <Color x:Key="Color.Surface0">#000000</Color>
  <Color x:Key="Color.Surface1">#101010</Color>
  <Color x:Key="Color.Surface2">#202020</Color>
  <Color x:Key="Color.SurfaceOverlay">#101010</Color>

  <Color x:Key="Color.TextPrimary">#FFFFFF</Color>
  <Color x:Key="Color.TextSecondary">#E0E0E0</Color>
  <Color x:Key="Color.TextTertiary">#C0C0C0</Color>
  <Color x:Key="Color.TextDisabled">#808080</Color>
  <Color x:Key="Color.TextOnAccent">#FFFFFF</Color>
  <Color x:Key="Color.TextOnDanger">#FFFFFF</Color>
  <Color x:Key="Color.TextLink">#4499FF</Color>

  <Color x:Key="Color.BorderDefault">#FFFFFF</Color>
  <Color x:Key="Color.BorderSubtle">#C0C0C0</Color>
  <Color x:Key="Color.BorderStrong">#FFFFFF</Color>
  <Color x:Key="Color.BorderFocus">#FFFF00</Color>
  <Color x:Key="Color.BorderError">#FF4444</Color>

  <Color x:Key="Color.AccentDefault">#0066FF</Color>
  <Color x:Key="Color.AccentHover">#4499FF</Color>
  <Color x:Key="Color.AccentPressed">#0044CC</Color>
  <Color x:Key="Color.AccentSubtle">#002244</Color>
  <Color x:Key="Color.AccentSubtleHover">#003366</Color>

  <Color x:Key="Color.DangerDefault">#DD0000</Color>
  <Color x:Key="Color.DangerHover">#FF4444</Color>
  <Color x:Key="Color.DangerPressed">#AA0000</Color>
  <Color x:Key="Color.DangerSubtle">#440000</Color>

  <Color x:Key="Color.ControlDefault">#101010</Color>
  <Color x:Key="Color.ControlHover">#202020</Color>
  <Color x:Key="Color.ControlPressed">#303030</Color>
  <Color x:Key="Color.ControlDisabled">#101010</Color>

  <Color x:Key="Color.SubtleDefault">#00000000</Color>
  <Color x:Key="Color.SubtleHover">#202020</Color>
  <Color x:Key="Color.SubtlePressed">#303030</Color>

  <Color x:Key="Color.SelectionBackground">#0066FF</Color>
  <Color x:Key="Color.SelectionText">#FFFFFF</Color>

  <Color x:Key="Color.OverlayScrim">#CC000000</Color>

  <!-- ── Brushes ── -->
  <SolidColorBrush x:Key="Brush.Background" Color="{DynamicResource Color.Background}" />
  <SolidColorBrush x:Key="Brush.Surface0" Color="{DynamicResource Color.Surface0}" />
  <SolidColorBrush x:Key="Brush.Surface1" Color="{DynamicResource Color.Surface1}" />
  <SolidColorBrush x:Key="Brush.Surface2" Color="{DynamicResource Color.Surface2}" />
  <SolidColorBrush x:Key="Brush.SurfaceOverlay" Color="{DynamicResource Color.SurfaceOverlay}" />

  <SolidColorBrush x:Key="Brush.TextPrimary" Color="{DynamicResource Color.TextPrimary}" />
  <SolidColorBrush x:Key="Brush.TextSecondary" Color="{DynamicResource Color.TextSecondary}" />
  <SolidColorBrush x:Key="Brush.TextTertiary" Color="{DynamicResource Color.TextTertiary}" />
  <SolidColorBrush x:Key="Brush.TextDisabled" Color="{DynamicResource Color.TextDisabled}" />
  <SolidColorBrush x:Key="Brush.TextOnAccent" Color="{DynamicResource Color.TextOnAccent}" />
  <SolidColorBrush x:Key="Brush.TextOnDanger" Color="{DynamicResource Color.TextOnDanger}" />
  <SolidColorBrush x:Key="Brush.TextLink" Color="{DynamicResource Color.TextLink}" />

  <SolidColorBrush x:Key="Brush.BorderDefault" Color="{DynamicResource Color.BorderDefault}" />
  <SolidColorBrush x:Key="Brush.BorderSubtle" Color="{DynamicResource Color.BorderSubtle}" />
  <SolidColorBrush x:Key="Brush.BorderStrong" Color="{DynamicResource Color.BorderStrong}" />
  <SolidColorBrush x:Key="Brush.BorderFocus" Color="{DynamicResource Color.BorderFocus}" />
  <SolidColorBrush x:Key="Brush.BorderError" Color="{DynamicResource Color.BorderError}" />

  <!-- High contrast uses solid borders — no gradient -->
  <SolidColorBrush x:Key="Brush.ControlElevationBorder" Color="{DynamicResource Color.BorderDefault}" />
  <SolidColorBrush x:Key="Brush.AccentElevationBorder" Color="{DynamicResource Color.AccentDefault}" />

  <SolidColorBrush x:Key="Brush.AccentDefault" Color="{DynamicResource Color.AccentDefault}" />
  <SolidColorBrush x:Key="Brush.AccentHover" Color="{DynamicResource Color.AccentHover}" />
  <SolidColorBrush x:Key="Brush.AccentPressed" Color="{DynamicResource Color.AccentPressed}" />
  <SolidColorBrush x:Key="Brush.AccentSubtle" Color="{DynamicResource Color.AccentSubtle}" />
  <SolidColorBrush x:Key="Brush.AccentSubtleHover" Color="{DynamicResource Color.AccentSubtleHover}" />

  <SolidColorBrush x:Key="Brush.DangerDefault" Color="{DynamicResource Color.DangerDefault}" />
  <SolidColorBrush x:Key="Brush.DangerHover" Color="{DynamicResource Color.DangerHover}" />
  <SolidColorBrush x:Key="Brush.DangerPressed" Color="{DynamicResource Color.DangerPressed}" />
  <SolidColorBrush x:Key="Brush.DangerSubtle" Color="{DynamicResource Color.DangerSubtle}" />

  <SolidColorBrush x:Key="Brush.ControlDefault" Color="{DynamicResource Color.ControlDefault}" />
  <SolidColorBrush x:Key="Brush.ControlHover" Color="{DynamicResource Color.ControlHover}" />
  <SolidColorBrush x:Key="Brush.ControlPressed" Color="{DynamicResource Color.ControlPressed}" />
  <SolidColorBrush x:Key="Brush.ControlDisabled" Color="{DynamicResource Color.ControlDisabled}" />

  <SolidColorBrush x:Key="Brush.SubtleDefault" Color="{DynamicResource Color.SubtleDefault}" />
  <SolidColorBrush x:Key="Brush.SubtleHover" Color="{DynamicResource Color.SubtleHover}" />
  <SolidColorBrush x:Key="Brush.SubtlePressed" Color="{DynamicResource Color.SubtlePressed}" />

  <SolidColorBrush x:Key="Brush.SelectionBackground" Color="{DynamicResource Color.SelectionBackground}" />
  <SolidColorBrush x:Key="Brush.SelectionText" Color="{DynamicResource Color.SelectionText}" />

  <SolidColorBrush x:Key="Brush.OverlayScrim" Color="{DynamicResource Color.OverlayScrim}" />

  <SolidColorBrush x:Key="Brush.SuccessDefault" Color="{DynamicResource Palette.Success500}" />
  <SolidColorBrush x:Key="Brush.WarningDefault" Color="{DynamicResource Palette.Warning500}" />
  <SolidColorBrush x:Key="Brush.WarningSubtle" Color="{DynamicResource Palette.Warning100}" />

  <!-- High contrast: gradients fall back to solid accent for maximum clarity -->
  <Color x:Key="Color.AccentViolet">#7C3AED</Color>
  <Color x:Key="Color.AccentRose">#EC4899</Color>

  <SolidColorBrush x:Key="Brush.AccentGradient" Color="{DynamicResource Color.AccentDefault}" />
  <SolidColorBrush x:Key="Brush.AccentGradientHover" Color="{DynamicResource Color.AccentHover}" />
  <SolidColorBrush x:Key="Brush.AccentGradientPressed" Color="{DynamicResource Color.AccentPressed}" />
  <SolidColorBrush x:Key="Brush.StratumLine" Color="{DynamicResource Color.AccentDefault}" />

  <!-- High contrast: selection gradient falls back to solid -->
  <SolidColorBrush x:Key="Brush.SelectionGradient" Color="{DynamicResource Color.AccentSubtle}" />

  <SolidColorBrush x:Key="Brush.SurfaceCard" Color="{DynamicResource Color.Surface1}" />
  <SolidColorBrush x:Key="Brush.SurfaceCardHover" Color="{DynamicResource Color.Surface2}" />
  <SolidColorBrush x:Key="Brush.AccentGlowSoft" Color="{DynamicResource Color.AccentSubtle}" />
  <SolidColorBrush x:Key="Brush.AccentGlowStrong" Color="{DynamicResource Color.AccentDefault}" />

</ResourceDictionary>
